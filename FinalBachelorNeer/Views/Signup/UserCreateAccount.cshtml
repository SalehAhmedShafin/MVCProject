MvcHtmlString ValidateMessage(Expression < Func<dynamic, TProperty>> expression, string validationMessage, object htmlAttributes)

@model FinalBachelorNeer.Models.userinfo


@{
    Layout = null;
}

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>BachelorNeer</title>

    <!-- Favicon -->
    <link rel="icon" href="../Design/img/favicon.png">

    <!-- Font awesome -->
    <link href="../Design/css/font-awesome.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="../Design/css/bootstrap.css" rel="stylesheet">
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="../Design/css/slick.css">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="../Design/css/nouislider.css">
    <!-- Fancybox slider -->
    <link rel="stylesheet" href="../Design/css/jquery.fancybox.css" type="text/css" media="screen" />
    <!-- Theme color -->
    <link id="switcher" href="../Design/css/theme-color/default-theme.css" rel="stylesheet">

    <!-- Main style sheet -->
    <link href="../Design/css/style.css" rel="stylesheet">


    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Vollkorn' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

    <script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script>

    <script src="//ajax.aspnetcdn.com/ajax/mvc/4.0/jquery.validate.unobtrusive.min.js"></script>

</head>
<body>
    <section id="aa-signin" style="background-image: url(../Design/img/signinbg.jpg)">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-signin-area">
                        <div class="aa-signin-form">
                            <div class="aa-signin-form-title">
                                <a class="aa-property-home" onclick="location.href='@Url.Action("Index","Home")'">Bachelor Neer</a>
                                <h4>Create your account and Stay with us</h4>
                            </div>
                            <form class="contactform" action="/Signup/UserCreateAccount" method="post">
                                <div class="aa-single-field">
                                    <label for="name">Name <span class="required">*</span></label>
                                    <input type="text" required="required" aria-required="true" value="" name="u_Name">
                                    @Html.ValidationMessageFor(m => m.u_Name)
                                </div>
                                <div class="aa-single-field">

                                    <label for="number">Contact Number <span class="required">*</span></label>

                                    <input type="text" required="required" aria-required="true" value="" name="u_Number">
                                    @Html.ValidationMessageFor(m => m.u_Number)
                                </div>


                                <div class="aa-single-field">
                                    <label for="email">Email <span class="required">*</span></label>
                                    <input type="email" required="required" aria-required="true" value="" name="u_Email">
                                    @Html.ValidationMessageFor(m => m.u_Email)
                                </div>

                                <div class="aa-single-field">
                                    <label for="nid">NID <span class="required">*</span></label>
                                    <input type="text" required="required" aria-required="true" value="" name="u_NID">
                                    @Html.ValidationMessageFor(m => m.u_NID)
                                </div>

                                <div class="aa-single-field">
                                    <label for="password">Password <span class="required">*</span></label>
                                    <input type="password" required="required" aria-required="true" value="" name="u_Password">
                                    @Html.ValidationMessageFor(m => m.u_Password)
                                </div>

                                <div class="aa-single-submit">
                                    <input type="submit" value="Create Account" name="Submit">
                                </div>

                                <div class="aa-single-field">
                                    @ViewBag.Error
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="../Design/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="../Design/js/bootstrap.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="../Design/js/slick.js"></script>
    <!-- Price picker slider -->
    <script type="text/javascript" src="../Design/js/nouislider.js"></script>
    <!-- mixit slider -->
    <script type="text/javascript" src="../Design/js/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="../Design/js/jquery.fancybox.pack.js"></script>
    <!-- Custom js -->
    <script src="../Design/js/custom.js"></script>

</body>
</html>